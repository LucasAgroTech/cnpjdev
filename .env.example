# Configuração das APIs
RECEITAWS_ENABLED=True
CNPJWS_ENABLED=True
CNPJA_OPEN_ENABLED=True
RECEITAWS_REQUESTS_PER_MINUTE=3
CNPJWS_REQUESTS_PER_MINUTE=3
CNPJA_OPEN_REQUESTS_PER_MINUTE=5

# Mantido para compatibilidade - Deve ser igual à soma das taxas individuais das APIs
REQUESTS_PER_MINUTE=11

# Configuração de controle de taxa
MAX_CONCURRENT_PROCESSING=6
API_COOLDOWN_AFTER_RATE_LIMIT=30
API_RATE_LIMIT_SAFETY_FACTOR=0.9

# Banco de dados
DATABASE_URL=postgresql://usuario:senha@host:porta/nome_banco

# Configuração da aplicação
DEBUG=False

# Configuração de persistência
AUTO_RESTART_QUEUE=True
MAX_RETRY_ATTEMPTS=3
